{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2018",
    "declaration": false,
    "module": "commonjs",
    "strictNullChecks": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "typeRoots": ["./src/@types", "./node_modules/@types"],
  },
  "noUnusedParameters": true,
  "noUnusedLocals": true,
  "pretty": true,
}
